  <DataSources>
    <DataSource Name="SCCMLive">
      <DataSourceReference>NewDB</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>cd2120fe-e25a-4c08-90e9-a89a1988bab2</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="MainDS">
      <Query>
        <DataSourceName>SCCMLive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Collection">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
Distinct 
		sys.AD_Site_Name0 as'AD Site'
		,Sys.Resource_Domain_OR_Workgr0 as'Domain'
		,sys.Name0 as'ServerName'
		,t.LastHardwareScan
        ,si.[Description]
       ,si.[Owner]
       ,CASE WHEN 
		   ClientState=5  THEN 'Configuration Manager, Windows update' 
		   WHEN ClientState=8  THEN 'Add or Remove Feature'
		   WHEN ClientState=12 THEN 'Windows Update, Add or Remove Feature'
		   WHEN ClientState=13 THEN 'Configuration Manager, Windows Update, Add or Remove Feature' 
		   WHEN (ClientState=0 or ClientState like null) then 'No pending reboot'
       end as Stat
      ,OPSYS.LastBootUpTime0 as'LastBootUpTime'
		,OPSYS.Caption0 as'OS'
		,m.SiteCode
		,CASE WHEN m.IsClient='1' THEN 'Yes' ELSE 'No' END IsClient
		,CASE WHEN m.IsActive='1' THEN 'Active' ELSE 'Inactive' END IsActive
from v_R_System sys
INNER JOIN v_FullCollectionMembership m on m.ResourceID=SYS.ResourceID
LEFT JOIN GlobalServerInventory.dbo.ServerInventory si on si.Hostname = sys.Name0 
LEFT JOIN vSMS_CombinedDeviceResources t on t.MachineID=SYS.ResourceID
LEFT JOIN v_GS_OPERATING_SYSTEM OPSYS on sys.ResourceID=OPSYS.ResourceID
WHERE m.CollectionID in (@Collection)</CommandText>
      </Query>
      <Fields>
        <Field Name="AD_Site">
          <DataField>AD Site</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Domain">
          <DataField>Domain</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastHardwareScan">
          <DataField>LastHardwareScan</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastBootUpTime">
          <DataField>LastBootUpTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OS">
          <DataField>OS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Owner">
          <DataField>Owner</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SiteCode">
          <DataField>SiteCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Stat">
          <DataField>Stat</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsClient">
          <DataField>IsClient</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsActive">
          <DataField>IsActive</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Coll_DS">
      <Query>
        <DataSourceName>SCCMLive</DataSourceName>
        <CommandText>SELECT coll.collectionid,coll.name,coll.collectionid + ' - ' + coll.name as [Collection] 
FROM v_FullCollectionMembership fcm
JOIN v_collection coll
ON  coll.collectionid=fcm.collectionid
GROUP BY coll.collectionid,coll.name 
ORDER BY 2</CommandText>
      </Query>
      <Fields>
        <Field Name="collectionid">
          <DataField>collectionid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Collection">
          <DataField>Collection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
